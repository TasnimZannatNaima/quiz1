<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ui.EndController"
            prefHeight="400.0" prefWidth="600.0" >
    <children>
        <Label fx:id="scoreLabel" layoutX="50" layoutY="50" prefWidth="300"/>
        <Button fx:id="playAgainBtn" layoutX="50" layoutY="120" text="Play Again" onAction="#onPlayAgain"/>
        <Button fx:id="exitBtn" layoutX="200" layoutY="120" text="Exit" onAction="#onExit"/>
    </children>

</AnchorPane>
